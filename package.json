{
  "name": "vite-plugin-vsharp",
  "version": "1.3.0",
  "description": "A Vite plugin that compresses static images after each builds by using sharp.js",
  "main": "./d/vsharp.js",
  "keywords": [
    "vite",
    "vite-plugin",
    "sharp.js"
  ],
  "scripts": {
    "dev": "npx babel vsharp.js --watch --out-file ./d/vsharp.js --no-comments",
    "build": "npx babel vsharp.js --out-file ./d/vsharp.js",
    "test-run": "npx vite build",
    "test-dev": "npx vite -d"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "vite": "^2.9.14"
  },
  "peerDependencies": {
    "vite": ">2.0.0-0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "glob": "^7.2.3",
    "mime-types": "^2.1.35",
    "sharp": "^0.30.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jw-12138/vite-plugin-vsharp"
  },
  "bugs": {
    "url": "https://github.com/jw-12138/vite-plugin-vsharp/issues"
  },
  "author": "Jacky Wong",
  "license": "MIT"
}
