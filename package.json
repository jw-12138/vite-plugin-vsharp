{
  "name": "vite-plugin-vsharp",
  "version": "1.1.2",
  "description": "A Vite plugin that compresses static images after each builds by using sharp.js",
  "main": "./d/vsharp.js",
  "keywords": [
    "vite",
    "vite-plugin",
    "sharp.js"
  ],
  "scripts": {
    "dev": "npx babel vsharp.js --watch --out-file ./d/vsharp.js",
    "build": "npx babel vsharp.js --out-file ./d/vsharp.js",
    "test-run": "npx vite build"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "vite": "^2.6.4"
  },
  "peerDependencies": {
    "vite": ">2.0.0-0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "glob": "^7.2.0",
    "sharp": "^0.29.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jw-12138/vite-plugin-vsharp"
  },
  "bugs": {
    "url": "https://github.com/jw-12138/vite-plugin-vsharp/issues"
  },
  "author": "Jacky Wong",
  "license": "MIT"
}
